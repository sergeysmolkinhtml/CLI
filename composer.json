{
  "require-dev": {
    "friendsofphp/php-cs-fixer": ">=2.16.3",
    "mockery/mockery": "^1.5",
    "phpunit/php-code-coverage": "^9.2",
    "phpstan/phpstan": "^1.9",
    "mikey179/vfsstream": "~1"
  },
  "autoload": {
    "psr-4": {
      "Main\\": "src/"
    }
  },
  "require": {
    "php-ds/php-ds": "^1.4",
    "phpunit/phpunit": "9.5",
    "illuminate/collections": "^9.40",
    "symfony/console": "^6.2"
  },
  "scripts": {
    "test": "phpunit",
    "coverage-html": "phpunit --coverage-html ./coverage/report",
    "html-coverage": [
      "php -n -dzend_extension=xdebug -dxdebug.mode=coverage ./vendor/bin/phpunit --coverage-text"
    ]
  }
}
